{
  "DBConnection": "Data Source=<HostName>\\<SqlInstanceName>;Database=<Database>;Integrated Security=SSPI;",
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console" ],
    "MinimumLevel": {
      "Default": "Warning",
      "Override": {
        "System": "Warning",
        "Microsoft": "Warning"
      }
    },
    "WriteTo": [
      { "Name": "Console" }
    ]
  },
  "CorsOrigins": "http://localhost:9999"
}